  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "lint": "ng lint",
    "local-dev": "firebase emulators:start --only firestore,auth,functions",
    "empty-emulator": "firebase emulators:start",
    "emulator": "firebase emulators:start --import test-data",
    "export": "firebase emulators:export test-data",
    "hosting-build": "ng build",
    "hosting-deploy": "ng build && firebase deploy --only hosting",
    "function-build": "cd functions && npm run lint && tsc && npm run build",
    "function-serve": "cd functions && npm run lint && tsc && npm run build && firebase emulators:start",
    "function-deploy": "cd functions && npm run lint && tsc && npm run build && firebase deploy --only functions --debug",
    "function-test": "cd functions && mocha --reporter spec",
    "deploy": "npm run function-deploy && npm run hosting-deploy"
  },
